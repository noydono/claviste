
{{#each dbArticle}}
<div class="parallax-container">
    <div class="parallax">
        <img src="{{img}}">
    </div>
</div>


<div class="card ">
    <div class="card-content">
        <span class="card-title">{{title}}</span>
        <p>{{content}}</p>
    </div>
    <div class="card-action">
        <a href="#">{{author}}</a>
        <a href="#">This is a link</a>
    </div>
</div>

{{/each}}





<script>

    // Or with jQuery

    $(document).ready(function () {
        $('.parallax').parallax();
    });
</script>