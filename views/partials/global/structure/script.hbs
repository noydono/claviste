<script src="/public/materialize-src/js/bin/materialize.min.js"></script>

<script>


    $(document).ready(function () {

        $('.tooltipped').tooltip();

        $('.dropdown-trigger').dropdown({
            closeOnClick: false
        });

        $('.materialboxed').materialbox();
        $('.collapsible').collapsible({
            accordion: true,
        });

        $('.sidenav').sidenav({
